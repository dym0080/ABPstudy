            <div class="progress-bar progress-bar-success" id="layout-loading-progress-bar" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width: 20%">
            </div>
        </div>
    </div>

    <div ng-include="'/App/views/layout/leftmenu.cshtml'"></div>



