                            <ul class="dropdown-menu dropdown-menu-default">
                                <li>
                                    <a href="javascript:void(0)" ng-click="vm.chgPassword();">
                                        <i class="icon-lock"></i> 修改密码
                                    </a>
                                </li>                                
                                <li class="divider"> </li>                                
                                <li>
                                    <a href="@Url.Action("Logout", "Account")"><i class="fa fa-sign-out"></i>退出系统</a>                                    
                                </li>
                            </ul>
                        </li>
                        <!-- END USER LOGIN DROPDOWN -->
                        <!-- BEGIN QUICK SIDEBAR TOGGLER -->
                       
                        <!-- END QUICK SIDEBAR TOGGLER -->
